{
  "name": "qr-front-v1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "servev18": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
